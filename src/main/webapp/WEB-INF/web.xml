<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>Medicine</servlet-name>
    <jsp-file>/medicine.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>Medicine</servlet-name>
    <url-pattern>/medicinePage</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddMedicine</servlet-name>
    <jsp-file>/addMed.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>AddMedicine</servlet-name>
    <url-pattern>/addMed</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>GetMedicine</servlet-name>
    <jsp-file>/getMed.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>GetMedicine</servlet-name>
    <url-pattern>/getMed</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Index</servlet-name>
    <jsp-file>/index.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/index.jsp</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Login</servlet-name>
    <jsp-file>/login.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/userLogin</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Register</servlet-name>
    <jsp-file>/register.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/userRegister</url-pattern>
  </servlet-mapping>

</web-app>
